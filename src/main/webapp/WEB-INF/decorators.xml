<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/resources/*</pattern>
	</excludes>

	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>

	<decorator name="login" page="login.jsp">
		<pattern>/*/login/*</pattern>
		<pattern>/login/*</pattern>
	</decorator>
</decorators>
